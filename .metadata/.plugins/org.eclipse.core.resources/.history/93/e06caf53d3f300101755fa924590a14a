package q4_EmployeePayment;

abstract public class Employee {
	private String name;
	private String empID;

	public String getName() {
		return name;
	}

	public String getEmpID() {
		return empID;
	}

	public Employee(String name, String empID) {
		this.name = name;
		this.empID = empID;
	}

	abstract int getPayment();
}
