package LabAssignment1;
/*
Generate the first 20 Fibonacci numbers using iterative logic.
Store all numbers in an array.
Print the numbers in one line.
Compute and print the average.
*/

public class Q1_Attempt1_Fibonacci {
    
	public static void main(String[] args) {
		int num=20;
		int[] fibonacci_series=new int[20];
		int k=2;
		fibonacci_series[0]=1;
		fibonacci_series[1]=1;
		int first=1;
		int second=1;
		System.out.println("The first 20 fibonacci numbers are : ")
		System.out.print(1+" "+1+" ");
		int sum=2;//sum of first two numbers of fibonacci series
		for(int i=2;i<20;i++) {
			fibonacci_series[k]=first+second;
			first=second;
			second=fibonacci_series[k];
			sum+=second;
		    System.out.print(second+" ");
			k++;
		}
		double average=(double)sum/20;
		System.out.println("Average: "+average);
	}
	
	
}
